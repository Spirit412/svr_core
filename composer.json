{
    "name": "svr/core",
    "description": "SVR CORE - A simple core package for SVR.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "homepage": "https://github.com/Spirit412/svr_core",
    "authors": [
        {
            "name": "Domanskiy Aleksandr",
            "email": "domanskiy.av@plinor.ru"
        }
    ],
    "autoload": {
        "psr-4": {
